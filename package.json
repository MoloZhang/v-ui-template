{
  "name": "v-ui-template",
  "sideEffects": false,
  "description": "Quickly create templates for vue sfc components",
  "version": "0.0.9",
  "repository": "https://github.com/ckpack/v-ui-template",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "style": "dist/index.css",
  "keywords": [
    "v-ui-template",
    "sfc",
    "rollup"
  ],
  "files": [
    "dist/*",
    "src/*",
    "libs/*"
  ],
  "scripts": {
    "serve": "yarn build && vue-cli-service serve dev/serve.js",
    "build": "rm -rf ./dist && rm -rf ./libs && rollup --config ./build/rollup.config.js",
    "lint": "eslint --fix --ext .js,.vue ./",
    "gen": "bash ./scripts/gc.sh"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "eslint": "^7.27.0",
    "eslint-plugin-vue": "^7.10.0",
    "husky": "^6.0.0",
    "postcss": "^8.3.0",
    "rollup": "^2.50.6",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.34.1",
    "vue": "3.1.0-beta.7"
  }
}
