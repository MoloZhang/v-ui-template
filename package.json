{
  "name": "v-ui-template",
  "sideEffects": false,
  "description": "Quickly create templates for vue sfc components",
  "version": "0.2.4",
  "repository": "https://github.com/ckpack/v-ui-template",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "style": "dist/index.css",
  "author": "chenkai",
  "keywords": [
    "v-ui-template",
    "sfc",
    "rollup"
  ],
  "files": [
    "dist/*",
    "src/*",
    "libs/*"
  ],
  "scripts": {
    "dev": "yarn build && vue-cli-service serve dev/serve.js",
    "build": "rm -rf ./libs && rm -rf ./dist && rollup --config ./build/rollup.config.js",
    "s:build": "yarn build && vue-cli-service build --dest website dev/serve.js",
    "lint": "eslint --fix --ext .js,.vue ./",
    "gen": "bash ./scripts/gc.sh"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.2.4",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.16.0",
    "husky": "^6.0.0",
    "postcss": "^8.3.6",
    "rollup": "^2.52.7",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "rollup-plugin-alias": "^2.2.0",
    "sass": "^1.38.0",
    "vue": "3.2.4",
    "glob": "^7.1.7"
  },
  "dependencies": {}
}
