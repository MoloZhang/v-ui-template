{
  "name": "v-ui-template",
  "sideEffects": false,
  "description": "Quickly create templates for vue sfc components",
  "version": "1.2.0",
  "repository": "https://github.com/ckpack/v-ui-template",
  "main": "dist/index.js",
  "module": "es/index.js",
  "unpkg": "dist/index.min.js",
  "author": "chenkai",
  "keywords": [
    "v-ui-template",
    "sfc",
    "rollup"
  ],
  "files": [
    "dist/*",
    "src/*",
    "es/*"
  ],
  "scripts": {
    "dev": "yarn build && vue-cli-service serve dev/serve.js",
    "build": "yarn gen:compoents && yarn build:es && yarn build:dist",
    "build:es": "rm -rf ./es && rollup --config ./build/build.es.js",
    "build:dist": "rm -rf ./dist && rollup --config ./build/build.dist.js",
    "build:style": "",
    "build:website": "vue-cli-service build --dest website dev/serve.js",
    "lint": "eslint --fix --ext .js,.vue ./",
    "gen": "node ./scripts/gen.js",
    "gen:compoents": "node ./scripts/gen-compoents.js",
    "release": "standard-version && git push &&  git push origin --tags",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.2.6",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.17.0",
    "glob": "^7.1.7",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "postcss-prefixer": "^2.1.3",
    "rollup": "^2.56.3",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.38.2",
    "standard-version": "^9.3.1",
    "vue": "3.2.6"
  },
  "peerDependencies": {
    "vue": "^3.2.6"
  },
  "dependencies": {},
  "engines": {
    "node": ">=12"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "eslint --fix"
    ]
  }
}
